{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "functions": {
    "app/**/*": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Surrogate-Control",
          "value": "no-store"
        },
        {
          "key": "X-Cache-Bust",
          "value": "2024-01-30-v3-final"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_CACHE_BUST": "2024-01-30-v3-final",
      "FORCE_REBUILD": "true"
    }
  },
  "cleanUrls": true,
  "trailingSlash": true
}